package br.com.fiap.inventi.beans;
/**
 * Classe com getters/setters do objeto usuario
 * */
public class UsuarioPj extends Usuario {
	
	private int codigo;
	private String cpf;
	
	
	
	
	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public String getCpf() {
		return cpf;
	}
	public void setCpf(String cpf) {
		this.cpf = cpf;
	}
	
}
