package br.com.fiap.inventi.beans;

public class UsuarioPf extends Usuario {
	private int codigo;
	private String cpf;
	
	public UsuarioPf(){
		
	}
	
	public String getAll(){
		return "Código Usuário PF: " + codigo + "\n" +
				"CPF:"
	}
	
	public int getCodigo() {
		return codigo;
	}
	public void setCodigo(int codigo) {
		this.codigo = codigo;
	}
	public String getCpf() {
		return cpf;
	}
	public void setCpf(String cpf) {
		this.cpf = cpf;
	}
	
	
}
